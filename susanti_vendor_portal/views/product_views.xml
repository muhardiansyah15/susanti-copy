<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_supplier_shipment_info" model="ir.ui.view">
        <field name="name">product.template.shipment.info.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <group name="purchase" position="after">
                <group name="shipment_info" string="Shipment Info">
                    <field name="shipment_from_id" strong="Route from"/>
                    <field name="shipment_to_id" strong="Route to"/>
                </group>
            </group>
        </field>
    </record>
</odoo>